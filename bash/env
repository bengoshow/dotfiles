export PATH=/usr/local/share/npm/bin:/usr/local/bin:/opt/subversion/bin:~/bin:~/bin/jsl-0.3.0-mac:/usr/local/git-tf:/usr/bin/java:/usr/local/git:~/git-repos/dotfiles/osx:$PATH

# Set iTerm Window Title
export PROMPT_COMMAND='echo -ne "\033]0;${PWD/#$HOME/~}\007"'

# Set Git Editor
export GIT_EDITOR='subl -w'

[[ -s "$HOME/.rvm/scripts/rvm" ]] && . "$HOME/.rvm/scripts/rvm"  # This loads RVM into a shell session.